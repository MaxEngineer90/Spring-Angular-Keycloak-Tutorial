{
  "dev": {
    "serverUrl": "http://localhost:8080/api/v1/greetings/greet",
    "Security": {
      "Auth": {
        "eternal": {
          "Type": "OAuth2",
          "Grant Type": "Password",
          "Client ID": "eternal_backend",
          "Token URL": "http://localhost/realms/eternal/protocol/openid-connect/token",
          "Username": "{{eternal-user}}",
          "Password": "{{eternal-password}}"
        }
      }
    }
  }
}